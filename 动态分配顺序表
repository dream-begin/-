#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<stdlib.h>
#define initsize 10
typedef struct//定义一个动态分配的顺序表
{
	int* data;
	int length;//表长
	int maxlength;//表的最大长度
}sqelist;
void initlist(sqelist& L)//初始化顺序表
{
	L.data = (int*)malloc(sizeof(int) * initsize);
		L.length = 0;
	L.maxlength = initsize;
}
void increasesize(sqelist &L, int len)//扩大顺序表空间
{
	int* p = L.data;
	L.data = (int*)malloc(sizeof(int) * (L.maxlength + len));
	for (int i = 0; i <L.length; i++)
	{
		L.data[i] = p[i];
	}
	L.maxlength = L.maxlength + len;
	free(p);//释放扩大前的存储空间
}
int main()
{
	sqelist L;
	initlist(L);
	//输入几个元素
	increasesize(L, 5);

}
