#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#define maxsize 10//定义数组最大值为10；
typedef struct {
	int date[maxsize];//用静态数组存放数据元素
	int length;//顺序表的当前长度
}sqlist;//顺序表的类型定义

//基本操作--初始化一个顺序表
void initlist(sqlist& L)
{
	for (int i = 0; i < maxsize; i++)
		L.date[i] = 0;//为顺序表的元素赋初值
	L.length = 0;//初始化顺序表长度
}
int main()
{
	sqlist L;//定义一个顺序表
	initlist(L);//初始化顺序表
	for (int i = 0; i < L.length; i++)//循环输出顺序表中元素
	{
		printf("datd[%d]=%d\n", i, L.date[i]);
		
	}
	return 0;
}
